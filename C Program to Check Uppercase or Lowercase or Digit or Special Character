#include <stdio.h>
int main()
{
    char c;

    printf("Enter any character : ");
    scanf("%c", &c);

    if (c >= 'A' && c <= 'Z')
    {
        printf("character is  an upper case character");
    }
    else if (c >= 'a' && c <= 'z')
    {
        printf("character is a lower case character");
    }
    else if (c >= '0' && c <= '9')
    {
        printf("it is digit");
    }
    else
    {
        printf("character is a special character");
    }
    return 0;
}
